    Name = "Fly keybind",
    CurrentKeybind = "F", 
    HoldToInteract = false,
    Flag = "FlyKeybind", 
    Callback = function(Keybind)
        if FLYING then
            chonk4891()
        else
            zorp4273()
        end
    end,
})

