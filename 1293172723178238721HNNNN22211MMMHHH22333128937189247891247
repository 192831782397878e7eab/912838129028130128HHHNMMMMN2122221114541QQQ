         end)

         game.StarterGui:SetCore("SendNotification", {
            Title = success and "Success!" or "Error!",
            Text = success and "Teleported successfully!" or "Teleport failed!",
            Duration = 3
         })
      else
         game.StarterGui:SetCore("SendNotification", {
            Title = "Error!",
            Text = "Invalid User ID! Please enter a valid ID.",
            Duration = 3
         })
      end
   end,
})

